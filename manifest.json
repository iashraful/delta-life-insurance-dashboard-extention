{
    "name": "Delta Life Dashboard Pro",
    "description": "This is the extention for delta life insurance portal.",
    "version": "1.0.1",
    "manifest_version": 3,
    "action": {
      "default_popup": "dashboard.html",
      "default_icon": "logo.png"
    },
    "permissions": [
      "webRequest",
      "activeTab"
    ],
    "host_permissions": [
      "*://*/*"
    ],
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
  }